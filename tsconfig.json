{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "bundler",
    "lib": [
      "ES2022"
    ],
    "allowJs": false,
    "checkJs": true,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": ".",
    "typeRoots": [
      "./@types"
    ],
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
